{
  "name": "resource-modules",
  "version": "1.0.1",
  "description": "",
  "main": "dist/cli.js",
  "bin": {
    "resource-modules": "./dist/cli.js"
  },
  "scripts": {
    "start": "flow-node lib/cli.js",
    "test": "standard && flow && flow-node ./node_modules/tape/bin/tape \"test/**/*.js\"",
    "type:coverage": "find . -name '*.js' -not -path \"./node_modules/*\"  -exec sh -c 'echo \"$0\"; flow coverage $0' {} \\;",
    "dist": "rm -rf dist/ && mkdir dist/ && find . -name '*.js' -path \"./lib/*\" -exec sh -c 'flow-remove-types --out-dir dist $0' {} \\; && mv dist/lib/* dist/ && rm -r dist/lib && chmod +x dist/cli.js"
  },
  "files": [
    "dist/",
    "resources.php"
  ],
  "author": "Joaquin Oltra <joaquin@chimeces.com> (http://chimeces.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint-plugin-flowtype": "^2.16.1",
    "flow-bin": "^0.32.0",
    "flow-remove-types": "^1.0.4",
    "standard": "^8.0.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "acorn": "^3.3.0",
    "deep-equal": "^1.0.1",
    "escodegen": "^1.8.1",
    "recursive-readdir": "^2.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "flow-typed/"
    ],
    "plugins": [
      "flowtype"
    ]
  }
}
